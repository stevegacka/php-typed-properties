{
    "name": "stevegacka/php-typed-properties",
    "description": "Add powerful support for property typing and mapping on PHP objects.",
    "keywords": [
        "stevegacka",
        "php-typed-properties"
    ],
    "homepage": "https://github.com/stevegacka/php-typed-properties",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Gacka",
            "email": "stephengacka@gmail.com",
            "homepage": "http://stevegacka.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Gacka\\TypedProperties\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gacka\\TypedProperties\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
